epal Length\"\n            required\n          ></v-text-field>\n          <v-text-field\n            v-model=\"sepalWidth\"\n            label=\"Sepal Width\"\n            required\n          ></v-text-field>\n          <v-text-field\n            v-model=\"petalLength\"\n            label=\"Petal Length\"\n            required\n          ></v-text-field>\n      <!--    <v-text-field\n            v-model=\"petalWidth\"\n            label=\"Petal Width\"\n            required\n          ></v-text-field>      -->\n          <v-select\n            :items=\"items\"\n            v-model=\"petalWidth\"\n            label=\"Petal width\"\n            dense\n            required\n          ></v-select>\n          <v-card-actions>\n            <v-btn color=\"success\" @click=\"submit\">Prediction\n            </v-btn>\n            <v-spacer></v-spacer>\n            <v-btn color=\"info\" @click=\"clear\">clear</v-btn>\n          </v-card-actions>\n         </form>\n        </v-card-text>  \n      </v-card>\n<br/>\n        <br/>\n<h1  class=\"text-md-center\" v-if=\"predictedClass\">Predicted Class is: {{ predictedClass }}</h1>\n<!-- END: IMPORTANT PART! -->\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n<script>\n  import axios from 'axios'\nexport default {\n    name: 'HelloWorld',\n    data: () => ({\n      sepalLength: '',\n      sepalWidth: '',\n      petalLength: '',\n      petalWidth: '',\n      predictedClass : '',\n      items: [\n        { text: '0.25' },\n        { text: '2.3' },\n        { text: '1.4' },\n        { text: '2.4' },\n        { text: '2.6' },\n      ],\n    }),\n    methods: {\n    submit () {\n      axios.post('http://0.0.0.0:8060/predict', {\n        sepal_length: this.sepalLength,\n        sepal_width: this.sepalWidth,\n        petal_length: this.petalLength,\n        petal_width: this.petalWidth\n      })\n      .then((response) => {\n        this.predictedClass = response.data.class\n      })\n    },\n    clear () {\n      this.sepalLength = ''\n      this.sepalWidth = ''\n      this.petalLength = ''\n      this.petalWidth = ''\n      this.$nextTick(() => {this.items = null })\n    }\n  }\n}\n</script>"]}]}